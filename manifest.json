{
  "manifest_version": 2,
  "name": "Spotify to Deezer",
  "version": "1.1.1",
  "description": "Open Spotify links in deezer",
  "applications": {
    "gecko": {
      "id": "{2252adaa-7951-448c-af30-439f588604c5}"
    }
  },
  "icons": {
    "400": "icons/spot-x-deezer-400x400.png",
    "48": "icons/spot-x-deezer-48x48.png"
  },
  "permissions": [
    "notifications",
    "contextMenus",
    "https://open.spotify.com/",
    "https://api.deezer.com/"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}