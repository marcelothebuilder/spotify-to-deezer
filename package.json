{
  "name": "spotify-to-deezer",
  "version": "1.2.0",
  "description": "Firefox extension",
  "main": "index.js",
  "author": "Marcelo Paix√£o Resende",
  "license": "GPLv3",
  "scripts": {
    "start": "webpack && web-ext --source-dir dist run",
    "run": "webpack && web-ext --source-dir dist run",
    "lint": "web-ext --source-dir dist lint",
    "sign": "webpack && web-ext --source-dir dist sign",
    "dev": "webpack --watch",
    "build": "webpack && web-ext --source-dir dist build --overwrite-dest",
    "build:production": "webpack --mode=production && web-ext --source-dir dist build --overwrite-dest",
    "zip": "git archive -o source.zip HEAD"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "web-ext": "^4.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "global": "^4.4.0",
    "readme-md-generator": "^1.0.0"
  }
}
